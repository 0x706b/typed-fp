  "main": "index.js",
  "scripts": {
    "start": "snowpack dev --secure",
    "postinstall": "concurrently 'npm:setup:ssl-cert' 'npm:setup:typescript-plugins'",
    "setup:ssl-cert": "npx devcert-cli generate localhost && mv localhost.cert snowpack.crt && mv localhost.key snowpack.key",
    "setup:typescript-plugins": "npx ts-patch install --persist"
  },
  "author": "Tylor Steinberger <tlsteinberger167@gmail.com>",
  "dependencies": {
    "uhtml": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@snowpack/plugin-webpack": "^2.1.0",
    "@types/react": "^16.9.52",
    "@types/react-dom": "^16.9.8",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "concurrently": "^5.3.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "snowpack": "^2.16.1",
    "webpack": "^4.44.2"
  }
}
