{
  "name": "@typed/fp",
  "version": "0.0.24",
  "description": "Data Structures and Resources for fp-ts",
  "sideEffects": false,
  "dependencies": {
    "@most/adapter": "^1.0.0",
    "@most/core": "^1.6.1",
    "@most/disposable": "^1.3.0",
    "@most/scheduler": "^1.3.0",
    "@most/types": "^1.1.0",
    "fp-ts": "^3.0.0-alpha.14",
    "ts-toolbelt": "^9.3.10",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@ampproject/remapping": "^1.0.1",
    "@typed/test": "^9.5.0",
    "@types/node": "^14.14.25",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "concurrently": "^5.3.0",
    "dts-bundle-generator": "^5.7.0",
    "esbuild": "^0.8.46",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-custom-alias": "^1.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^5.0.9",
    "lint-staged": "^10.5.4",
    "magic-string": "^0.25.7",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "typescript-transform-paths": "^2.2.3"
  },
  "scripts": {
    "--maintenance--": "",
    "build": "npm run check:ts-patch && npx concurrently 'npm:build:typescript' 'npm:build:dts'",
    "postbuild": "npm run update",
    "build:typescript": "ts-node ./tools/build.ts",
    "build:dts": "ts-node ./tools/dts.ts",
    "update": "concurrently 'npm:update:gitignore'",
    "update:gitignore": "ts-node ./tools/update-gitignore.ts",
    "check:ts-patch": "ts-node ./tools/checkTsPatchIsActive",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "unit-test": "typed-test",
    "test": "concurrently 'npm:lint' 'npm:unit-test'",
    "precommit": "lint-staged",
    "preversion": "npm test && npm run update && npm run build",
    "postversion": "npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TylorS/typed-fp.git"
  },
  "keywords": [
    "TypeScript",
    "Typed",
    "fp-ts",
    "fp"
  ],
  "author": "Tylor Steinberger <tlsteinberger167@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TylorS/typed-fp/issues"
  },
  "homepage": "https://github.com/TylorS/typed-fp#readme"
}
