{
  "name": "@typed/fp",
  "version": "0.0.0",
  "description": "Data Structures and Resources for fp-ts",
  "sideEffects": false,
  "dependencies": {
    "@most/adapter": "^1.0.0",
    "@most/core": "^1.6.1",
    "@most/disposable": "^1.3.0",
    "@most/scheduler": "^1.3.0",
    "@most/types": "^1.1.0",
    "fp-ts": "^2.8.5",
    "io-ts": "^2.2.12",
    "logging-ts": "^0.3.4",
    "monocle-ts": "^2.3.3",
    "newtype-ts": "^0.3.4",
    "tslib": "^2.0.3",
    "ts-toolbelt": "^8.0.7"
  },
  "devDependencies": {
    "@typed/test": "^9.5.0",
    "@types/node": "^14.14.6",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "@zoltu/typescript-transformer-append-js-extension": "^1.0.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-custom-alias": "^1.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "fast-glob": "^3.2.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "snowpack": "^2.17.0",
    "ts-morph": "^8.1.2",
    "ts-node": "^9.0.0",
    "ts-patch": "^1.3.0",
    "ts-transformer-replace-paths": "^1.0.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.1.1-rc"
  },
  "scripts": {
    "--maintenance--": "",
    "build": "npm run check:ts-patch && npm run build:typescript && npm run build:packages",
    "build:packages": "ts-node ./tools/build-packages",
    "build:packages:cjs": "ts-node ./tools/build-packages --cjs",
    "build:packages:esm": "ts-node ./tools/build-packages --esm",
    "build:typescript": "concurrently 'npm:build:typescript:cjs' 'npm:build:typescript:esm'",
    "build:typescript:cjs": "tsc -b ./src/tsconfig.cjs.json --incremental",
    "build:typescript:esm": "tsc -b ./src/tsconfig.esm.json --incremental",
    "check:ts-patch": "ts-node ./tools/checkTsPatchIsActive",
    "clean": "npm run clean:build && npm run clean:typescript",
    "clean:build": "ts-node ./tools/cleanup",
    "clean:typescript": "concurrently 'npm:clean:typescript:cjs' 'npm:clean:typescript:esm'",
    "clean:typescript:cjs": "npm run build:typescript:cjs -- --clean",
    "clean:typescript:esm": "npm run build:typescript:esm -- --clean",
    "docs": "ts-node ./node_modules/.bin/docs-ts",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "unit-test": "typed-test",
    "test": "concurrently 'npm:lint' 'npm:unit-test'",
    "test:watch": "npm test -- --watch",
    "update": "concurrently 'npm:update:gitignore' 'npm:update:tsconfig'",
    "update:gitignore": "ts-node ./tools/update-gitignore",
    "update:tsconfig": "ts-node ./tools/update-tsconfigs",
    "preversion": "npm test && npm run update && npm run build",
    "postversion": "npm publish",
    "watch": "concurrently 'npm:watch:cjs' 'npm:watch:esm'",
    "watch:cjs": "tsc-watch --build --incremental tsconfig.cjs.json --onSuccess 'npm run build:packages'",
    "watch:esm": "tsc-watch --build --incremental tsconfig.esm.json --onSuccess 'npm run build:packages'",
    "--helpers--": "",
    "postinstall": "ts-patch install --persist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TylorS/typed-fp.git"
  },
  "keywords": [
    "TypeScript",
    "Typed",
    "fp-ts",
    "fp"
  ],
  "author": "Tylor Steinberger <tlsteinberger167@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TylorS/typed-fp/issues"
  },
  "homepage": "https://github.com/TylorS/typed-fp#readme"
}
