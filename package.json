  "main": "index.js",
  "dependencies": {
    "@most/core": "^1.5.0",
    "@most/disposable": "^1.2.2",
    "@most/hold": "^4.1.0",
    "@most/scheduler": "^1.2.3",
    "@most/types": "^1.0.2",
    "@typed/promises": "^5.3.0",
    "fp-ts": "^2.8.1",
    "io-ts": "^2.2.9",
    "monocle-ts": "^2.3.3",
    "most-subject": "^6.0.0",
    "newtype-ts": "^0.3.4",
    "path-to-regexp": "^6.1.0",
    "ts-toolbelt": "^6.15.1"
  },
  "devDependencies": {
    "@typed/test": "^9.5.0",
    "@types/node": "^14.0.27",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "tslib": "^2.0.1",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "--maintenance--": "",
    "build": "npm run cleanup && tsc -P ./tsconfig.build.json",
    "cleanup": "npm run tsnode -- ./tools/cleanup",
    "lint": "eslint source --ext .js,.jsx,.ts,.tsx --fix",
    "unit-test": "npm run tsnode -- ./node_modules/.bin/typed-test",
    "test": "npm run lint && npm run unit-test",
    "test:watch": "npm test -- --watch",
    "update:gitignore": "npm run tsnode --  ./tools/update-gitignore",
    "update": "npm run update:gitignore",
    "preversion": "npm test && npm run build",
    "postversion": "npm run update && npm publish",
    "--helpers--": "",
    "tsnode": "node --loader ts-node/esm.mjs --experimental-specifier-resolution=node "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TylorS/typed-fp.git"
  },
  "keywords": [
    "TypeScript",
    "Typed",
    "fp-ts",
    "fp"
