{
  "compilerOptions": {
    "outDir": "../esm",
    "module": "esnext",
    "plugins": [
      {
        "transform": "@zerollup/ts-transform-paths",
        "after": true
      },
      {
        "transform": "@zoltu/typescript-transformer-append-js-extension/output/index.js",
        "after": true
      },
      {
        "transform": "ts-transform-import-path-rewrite",
        "import": "transform",
        "alias": {
          "^fp-ts/(.+)": "fp-ts/es6/$1"
        },
        "after": true,
        "type": "config"
      }
    ]
  },
  "extends": "../tsconfig.base.json",
  "files": [],
  "includes": [
    "./exports.ts"
  ],
  "references": [
