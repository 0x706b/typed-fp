    },
    {
      "path": "./Disposable/tsconfig.esm.json"
    },
    {
      "path": "./Effect/tsconfig.esm.json"
    },
    {
      "path": "./Future/tsconfig.esm.json"
    },
    {
      "path": "./Key/tsconfig.esm.json"
    },
    {
      "path": "./Op/tsconfig.esm.json"
    },
    {
      "path": "./Path/tsconfig.esm.json"
    },
    {
