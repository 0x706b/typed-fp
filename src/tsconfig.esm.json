    },
    {
      "path": "./SharedRef/tsconfig.esm.json"
    },
    {
      "path": "./Storage/tsconfig.esm.json"
    },
    {
      "path": "./Stream/tsconfig.esm.json"
    },
    {
      "path": "./Uri/tsconfig.esm.json"
    },
    {
