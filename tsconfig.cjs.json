{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "outDir": "cjs",
    "module": "commonjs",
    "plugins": [
      {
        "transform": "@zerollup/ts-transform-paths",
        "after": true
      },
      {
        "transform": "@zerollup/ts-transform-paths",
        "afterDeclarations": true
      }
    ]
  },
  "exclude": [
    "examples",
    "node_modules",
    "src/**/*.test.ts",
    "src/**/*.browser-test.ts",
    "tools",
    "cjs",
    "esm",
    "build",
    "index.js",
    "index.d.ts",
    "index.js.map",
    "index.d.ts.map",
    "Adapter",
    "AsyncData",
    "Branded",
    "Disposable",
    "Env",
    "EnvEither",
    "EnvOption",
    "Eq",
    "Fail",
    "Fiber",
    "FromEnv",
    "FromResume",
    "Fx",
    "FxT",
    "Global",
    "Hkt",
    "IOEither",
    "MonadRec",
    "Option",
    "Patch",
    "Provide",
    "Queue",
    "Reader",
    "ReaderEither",
    "ReaderStream",
    "ReaderStreamEither",
    "ReaderTask",
    "ReaderTaskEither",
    "Ref",
    "RefAdapter",
    "RefMap",
    "RefQueue",
    "RefWeakMap",
    "Resume",
    "ResumeEither",
    "ResumeOption",
    "Scheduler",
    "State",
    "StateEnvEither",
    "StateReaderTaskEither",
    "Stream",
    "StreamEither",
    "StreamOption",
    "Task",
    "TaskEither",
    "TaskOption",
    "WeakMap",
    "function",
    "hooks"
  ],
  "files": [],
  "include": [
    "src"
  ],
  "references": []
}
